{
  "name": "react-flag-icon-css-example-multi",
  "version": "1.0.0",
  "description": "Example project for React Flag Icon Css",
  "main": "server.js",
  "keywords": [
    "react",
    "react-flag-icon-css",
    "flag-icon-css",
    "flag-icon",
    "flag-icons",
    "react-css-modules"
  ],
  "author": {
    "name": "matteocng",
    "email": "matteocng+npmjs@outlook.com",
    "url": "https://github.com/matteocng"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/matteocng/react-flag-icon-css-example-multi.git"
  },
  "scripts": {
    "start": "cross-env USE_CSS_MODULES=true NODE_ENV=development babel-node webpack/server.js",
    "start:classes": "cross-env USE_CSS_MODULES=false NODE_ENV=development babel-node webpack/server.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src webpack cmrh.conf.js",
    "lint:css": "sass-lint 'src/**/*.scss' -v -q",
    "lint:fix": "eslint src webpack --fix",
    "pre-commit": "npm run lint && npm run flow"
  },
  "pre-commit": [
    "lint:js",
    "lint:css",
    "flow"
  ],
  "dependencies": {
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-css-modules": "^4.0.5",
    "react-dom": "^16.0.0",
    "react-flag-icon-css": "^1.0.23",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-react-app": "^2.2.0",
    "babel-runtime": "^6.23.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.57.3",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.3.3",
    "pre-commit": "^1.2.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.18.1",
    "webpack-dev-server": "^1.16.5"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
